<configuration>

    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%d{HH:mm:ss.SSS} [%thread] %-5level %logger{48}:%L - %msg%n</pattern>
        </encoder>
    </appender>

    <logger name="com.appmetr.s2s" level="trace"/>

    <root level="warn">
        <appender-ref ref="STDOUT" />
    </root>

</configuration>
